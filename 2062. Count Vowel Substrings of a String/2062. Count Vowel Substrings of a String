class Solution {
    public int countVowelSubstrings(String word) {
       
    int n = word.length(), count = 0;
    for (int i = 0; i < n; i++) {
        boolean[] seen = new boolean[5];
        int unique = 0;
        for (int j = i; j < n; j++) {
            char c = word.charAt(j);
            int idx = "aeiou".indexOf(c);
            if (idx == -1) break;
            if (!seen[idx]) {
                seen[idx] = true;
                unique++;
            }
            if (unique == 5) count++;
        }
    }
    return count;
}

    }
