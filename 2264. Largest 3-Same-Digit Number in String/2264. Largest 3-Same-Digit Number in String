class Solution {
    public String largestGoodInteger(String num) {
        String ans= "";
        for (int i = 0; i < num.length() - 2; i++) {
    char a = num.charAt(i);
    char b = num.charAt(i + 1);
    char c = num.charAt(i + 2);
    if (a == b && b == c) {
        String s = num.substring(i, i + 3);
        if (s.compareTo(ans) > 0) {
            ans = s;
        }
    }
}
return ans;

        
    }
}
