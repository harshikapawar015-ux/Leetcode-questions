class Solution {
    public int mostFrequentEven(int[] nums) {

        int[] freq = new int[100001];
    for (int x : nums) {
        if (x % 2 == 0) freq[x]++;
    }
    int ans = -1, max = 0;
    for (int i = 0; i < freq.length; i += 2) {
        if (freq[i] > max) {
            max = freq[i];
            ans = i;
        }
    }
    return ans;
}
        
    }
